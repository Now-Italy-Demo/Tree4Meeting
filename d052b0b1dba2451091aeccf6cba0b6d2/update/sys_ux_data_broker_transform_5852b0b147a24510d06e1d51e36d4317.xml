        <script><![CDATA[function transform(input) {
    var records = input.graphqlData.data.GlideRecord_Query.x_snc_tree4meeting_event._results;

    var events = [];

    for (var i = 0; i < records.length; i++) {
		var dateCleansed = new EventRegistrationDTformat().formatDateTime(records[i].date_from.displayValue);
		events.push({
			"sys_id": records[i].sys_id.value,
			"event_name": records[i].event_name.value,
			"attendees": records[i].attendees.value,
			"date_from": dateCleansed,
			"capacity": records[i].capacity.value,
			"spaces_left": records[i].spaces_left.value,
			"table": records[i].sys_class_name.value
		});
    }
    return events;
}]]></script>
