        <http_method>GET</http_method>
        <name>GET</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

	var gr = new GlideRecord('x_snc_tree4meeting_event');
	gr.query();
	var count = 0;
	while(gr.next()){

		var grp = new GlideRecord('x_snc_tree4meeting_compensazione');
		grp.addQuery('nome_evento', gr.event_name);
		grp.query();
		var exists = false; 
		while(grp.next()){
			exists = true;
		}
		if( !exists ) {
			var gr_comp = new GlideRecord('x_snc_tree4meeting_compensazione');
			gr_comp.initialize(); 
			gr_comp.nome_evento = gr.event_name; 
			gr_comp.insert();
			count++;
		}

	}
	
	return { 'inserted': count };

})(request, response);]]></operation_script>
