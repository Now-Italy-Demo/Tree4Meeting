        <http_method>GET</http_method>
        <name>CleanAll</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var gr_comp = new GlideRecord('x_snc_tree4meeting_compensazione');
    gr_comp.query();

    var comp_status = 'none';
    while (gr_comp.next()) {
        gr_comp.deleteRecord();
        comp_status = 'deleted';
    }

    var gr_part = new GlideRecord('x_snc_tree4meeting_attendee');
    gr_part.query();

    var part_status = 'none';
    while (gr_part.next()) {
        gr_part.deleteRecord();
        part_status = 'deleted';
    }

    return {
        'Compensation: ': comp_status,
        'Participants: ': part_status
    };

})(request, response);]]></operation_script>
